# isrExposureOutput.paf

parameters: {
    outputItems: {
        isrExposure: {
            datasetId: {
                datasetType: postISR
                fromClipboard: "name" "visit" "ccd" "snap" "filter" "channel"
            }
            storagePolicy: {
               storage: "FitsStorage"
               location: "%(output)/postISR/v%(visit)-f%(filter)/s%(snap)/c%(ccd)/a%(channel).fits"
            }
        }
    }
}
